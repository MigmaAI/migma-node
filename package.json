{
  "name": "migma",
  "version": "1.0.4",
  "description": "Official Node.js SDK for the Migma email platform API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/ --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:live": "tsx test-live.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "migma",
    "email",
    "api",
    "sdk",
    "email-marketing",
    "email-generation",
    "ai-email"
  ],
  "author": "Migma <hello@migma.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MigmaAI/migma-node"
  },
  "homepage": "https://docs.migma.ai",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  }
}
